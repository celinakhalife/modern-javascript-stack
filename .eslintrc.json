{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
      ],
    "plugins": [
        "flowtype",
        "compat"
      ],
      "settings": {
        "polyfills": ["fetch"]
      },
    "env": {
        "jest": true,
        "browser": true
    },
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "compat/compat": 2
      }
  }
